<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>SwitchSimulator.html</welcome-file>
	</welcome-file-list>

	<!-- Servlets -->
	<servlet>
		<servlet-name>switchInfoServiceImpl</servlet-name>
		<servlet-class>manatee.demo.switchsimulator.server.SwitchSimulatorServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>switchInfoServiceImpl</servlet-name>
		<url-pattern>/switchsimulator/simulatorControl</url-pattern>
	</servlet-mapping>

</web-app>